  "version": "0.2.2",
  "author": "Kazuyuki Honda <hakobera@gmail.com>",
  "dependencies": {
    "tuppari": "0.1.1",
    "router-line": "0.2.1",
    "node-uuid": "1.3.3",
    "dynamo": "0.2.13",
    "async": "0.1.22",
    "debug": "0.7.0",
    "aws2js": "0.7.0"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "sinon": "*"
  },
  "keyword": [
    "tuppari",
    "WebSocket",
    "aws"
  ],
  "repository": "git://github.com/hakobera/tuppari-servers",
  "scripts": {
